{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "email",
        "password"
    ],
    "properties": {
        "email": {
            "type": "string",
            "minLength": 6,
            "maxLength": 100,
            "format": "email",
            "default": ""
        },
        "password": {
            "type": "string",
            "minLength": 5,
            "maxLength": 20,
            "default": "",
            "title": "The password Schema",
            "examples": [
                "string"
            ]
        }
    },
    "examples": [{
        "email": "testadmin@gmail.com",
        "password": "string"
    }]
}